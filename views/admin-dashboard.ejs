<%- include('partials/header') %>

<h1>Contact Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Message</th>
      <th>Submitted At</th>
    </tr>
  </thead>
  <tbody>
    <% contacts.forEach(contact => { %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.message %></td>
        <td><%= new Date(contact.created_at).toLocaleString() %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('partials/footer') %>